						<control type="label">
							<description>Now Playing</description>
							<posx>0</posx>
							<posy>-5</posy>
							<width>700</width>
							<label>[COLOR Accent]$INFO[VideoPlayer.Title][/COLOR] [LOWERCASE]$INFO[VideoPlayer.Year][/LOWERCASE]</label>
							<scroll>true</scroll>
							<font>Home_menu</font>
							<align>left</align>
